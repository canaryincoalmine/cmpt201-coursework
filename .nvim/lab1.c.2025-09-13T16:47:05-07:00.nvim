#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  printf("Please enter some text: ");
  char *input = NULL;
  char size = 0;
  ssize_t num_char = getline(&input, &size, stdin);

  if (num_char == -1) {
    perror("getline failed");
    exit(EXIT_FAILURE);
  }

  char *pointer;
  char *output = strtok_r(input, " ", &pointer);

  while (output != NULL) {
    printf("%s\n", output);
    output = strtok_r(NULL, " ", &pointer);
  }
  free(input);

  return 0;
}
